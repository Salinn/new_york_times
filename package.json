        {
            "from": "node_modules/@webcomponents/webcomponentsjs/webcomponents-bundle.js",
            "to": "public/vendor/webcomponents-bundle.js"
        }
    ],
    "jest": {
        "collectCoverageFrom": [
            "**/*.js",
            "!**/node_modules/**",
            "!**/api/**",
            "!**/assets/**",
            "!**/build/**",
            "!**/coverage/**",
            "!**/store/**",
            "!**/vendor/**",
            "!**/index.js",
            "!**/initialState.js",
            "!**/ActionTypes.js",
            "!**/index.js",
            "!**/registerServiceWorker.js",
            "!**/initialState.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    }
